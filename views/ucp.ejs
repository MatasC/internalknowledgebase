  <body style = "padding-bottom:200px;">
    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
      <div class="container">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="#add_category">Categories</a>
            </li>
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="#add_item">Items</a>
            </li>
            <% if (user && user.access === 1){ %>
                <li class="nav-item">
                    <a class="nav-link js-scroll-trigger" href="#add_icon">Icons</a>
                </li>
            <% } %>
          </ul>
        </div>
      </div>
    </nav>

    <% if (user && user.access === 1){%>
        <%- include ('ucp_sections/admin.ejs') %>
    <%}else if(user && user.access === 0){%>
        <%- include ('ucp_sections/agent.ejs')%>
    <%}%>

    <div class = "scrollToToppp">
        <span class = "pull-right">
            <a href = "/" target = "">
                <button type="button" class = "btn btn-info"><i class = "fa fa-lg fa-home"></i></button>
            </a>
        </span>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
        <!-- Plugin JavaScript -->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom JavaScript for this theme -->
    <script src="js/scrolling-nav.js"></script>

    <script>
        $(document).ready(function() {
            $('#checkChangedbutton').on('keyup change', 'input, select, textarea', function(){

                var title = $('#item_title').val();
                var icon = $('#item_icon').val();

                $("#item_preview_md").html('<button class = "btn btn-dark item" id = "item_btn_preview" style ="width:180px !important; height:180px !important; overflow-wrap: break-word;">'+
                        '<h4><i class="fa fa-'+ icon +' fa-lg" id = "item_icon_preview" aria-hidden="true"></i></h4>'+
                        '<p id = "item_text">'+ title +'</p>'+
                    '</button>');
            });
        });

        (function(i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function() {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-139850820-1', 'auto');
        ga('set', 'checkProtocolTask', function() {});
        ga('require', 'displayfeatures');

        $('#submit_cat').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Submit Category',
            'eventLabel': 'Submit Category',
            'eventValue': 1
          });
        })

        $('#delete_cat').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Delete Category',
            'eventLabel': 'Delete Category',
            'eventValue': 1
          });
        })

        $('#submit_item').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Submit Item',
            'eventLabel': 'Submit Item',
            'eventValue': 1
          });
        })

        $('#delete_item').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Delete Item',
            'eventLabel': 'Delete Item',
            'eventValue': 1
          });
        })

        $('#submit_icon').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Submit Icon',
            'eventLabel': 'Submit Icon',
            'eventValue': 1
          });
        })

        $('#submit_custom_cat').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Submit Custom Category',
            'eventLabel': 'Submit Custom Category',
            'eventValue': 1
          });
        })

        $('#submit_custom_item').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Submit Custom Item',
            'eventLabel': 'Submit Custom Item',
            'eventValue': 1
          });
        })

        $('#delete_custom_cat').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Delete Custom Category',
            'eventLabel': 'Delete Custom Category',
            'eventValue': 1
          });
        })

        $('#delete_custom_item').click(()=>{
          ga('send', 'event', {
            'eventCategory': 'SuperCS',
            'eventAction': 'Delete Custom Item',
            'eventLabel': 'Delete Custom Item',
            'eventValue': 1
          });
        })

       
    </script>
  </body>

</html>
